{
  "rewrites": [
    { 
      "source": "/api/:path*", 
      "destination": "/api/:path*" 
    },
    {
      "source": "/__/auth/(.*)",
      "destination": "https://guide-co.firebaseapp.com/__/auth/$1"
    },
    { 
      "source": "/(.*)", 
      "destination": "/index.html" 
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "create-react-app"
}
